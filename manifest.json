{
  "manifest_version": 2,

  "name": "ERT Session Handler",
  "description": "Removes session idle time when working with GSSO.",
  "version": "1.0",

  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "background": {
    "scripts": ["session.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.ert.com/*"
  ]
}